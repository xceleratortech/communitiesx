{
    "name": "rstart",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbopack",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "format": "prettier --write .",
        "prepare": "husky install",
        "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
        "db:push": "drizzle-kit push --config=drizzle.config.ts",
        "db:auth:generate": "pnpm dlx @better-auth/cli generate --output src/server/db/auth-schema.ts -y",
        "db:seed": "pnpm dlx tsx --env-file .env.local src/server/db/seed.ts",
        "update:community-roles": "pnpm dlx tsx --env-file .env.local scripts/update-community-roles.ts",
        "import:users": "pnpm dlx tsx --env-file .env.local scripts/import-users.ts"
    },
    "dependencies": {
